AuthType WebAuth
Require valid-user

<IfModule mod_rewrite.c>
  DirectoryIndex index.php
  RewriteEngine on

  #RewriteBase /group/paperless2/cgi-bin/
  #RewriteBase /
 
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule ^(.*)$ index.php?path=/$1 [NC,QSA]
</IfModule>
